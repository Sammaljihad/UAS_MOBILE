<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tokoapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/config_network"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.example.tokoapp.activity.BaruActivity"/>
        <activity android:name="com.example.tokoapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.tokoapp.activity.LoginActivity"/>
        <activity android:name="com.example.tokoapp.activity.RegisterActivity" />
        <activity android:name="com.example.tokoapp.activity.DetailProdukActivity" />
        <activity android:name="com.example.tokoapp.activity.PengirimanActivity" />
        <activity android:name="com.example.tokoapp.activity.ListAlamatActivity" />
        <activity android:name="com.example.tokoapp.activity.TambahAlamatActivity" >

        </activity>
        <activity android:name="com.example.tokoapp.activity.PembayaranActivity" />
        <activity android:name="com.example.tokoapp.activity.SuccessActivity" />
        <activity android:name="com.example.tokoapp.activity.RiwayatActivity" />

    </application>

</manifest>